        <div class="col-md-4"></div>
        <div class="col-md-4">


            {{#each users}}
              <h2>{{username}}</h2>
              <h3>{{monsterType}}</h3>
              <img src={{imageURL}} id="allUserPics">
              <hr id="break">
            {{/each}}
        </div>
        <div class="col-md-4"></div>
    </div>
    <div class="col-md-2 sideCols2"></div>
  </div>

</div>



